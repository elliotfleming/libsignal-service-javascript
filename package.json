{
  "name": "@throneless/libsignal-service",
  "version": "5.0.0",
  "description": "A fork of the the libtextsecure components of Signal-Desktop, adapted for use by nodejs.",
  "main": "dist/libsignal-service.js",
  "files": [
    "/src",
    "/config",
    "/protos"
  ],
  "scripts": {
    "clean": "rimraf dist/*.js",
    "build:protobuf": "pbjs --target static-module --wrap es6 -o dist/compiled.js protos/*.proto",
    "build:barrelsby": "barrelsby --delete -d src/shims",
    "build:webpack": "webpack",
    "build": "npm run build:protobuf && npm run build:barrelsby && npm run build:webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/throneless-tech/libsignal-service-javascript.git"
  },
  "keywords": [
    "signal",
    "crypto",
    "textsecure",
    "axolotl"
  ],
  "author": "Josh King <josh@throneless.tech>",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/throneless-tech/libsignal-service-javascript/issues"
  },
  "homepage": "https://github.com/throneless-tech/libsignal-service-javascript#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@sindresorhus/is": "0.8.0",
    "abort-controller": "3.0.0",
    "browserify-zlib": "^0.2.0",
    "btoa": "^1.2.1",
    "bytebuffer": "^5.0.1",
    "config": "1.28.1",
    "crypto-browserify": "^3.12.0",
    "debug": "^4.1.1",
    "encoding": "0.1.13",
    "google-libphonenumber": "3.2.17",
    "https-browserify": "^1.0.0",
    "image-size": "^0.7.5",
    "libsignal-client": "https://github.com/signalapp/libsignal-client-node.git#f10fbd04eb6efb396eb12c25429761e8785dc9d0",
    "lodash": "4.17.20",
    "long": "^4.0.0",
    "memoizee": "0.4.14",
    "mime-types": "^2.1.24",
    "moment": "2.21.0",
    "node-fetch": "2.6.1",
    "node-forge": "0.10.0",
    "node-webcrypto-ossl": "2.1.2",
    "os-browserify": "^0.3.0",
    "p-props": "4.0.0",
    "p-queue": "6.2.1",
    "path-browserify": "^1.0.1",
    "protobufjs": "6.10.2",
    "proxy-agent": "^4.0.1",
    "ringrtc": "https://github.com/signalapp/signal-ringrtc-node.git#b43c6b728d62b6d386d95705e128f32f44edb650",
    "semver": "^7.3.5",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.1.1",
    "uuid": "3.3.2",
    "vm-browserify": "^1.1.2",
    "websocket": "1.0.28",
    "zkgroup": "https://github.com/signalapp/signal-zkgroup-node.git#2d7db946cc88492b65cc66e9aa9de0c9e664fd8d"
  },
  "devDependencies": {
    "@types/bytebuffer": "^5.0.42",
    "@types/debug": "^4.1.5",
    "@types/google-libphonenumber": "7.4.14",
    "@types/lodash": "4.14.106",
    "@types/long": "4.0.1",
    "@types/memoizee": "0.4.2",
    "@types/node": "10.17.58",
    "@types/node-fetch": "2.5.7",
    "@types/node-forge": "0.9.5",
    "@types/uuid": "3.4.4",
    "@types/webpack-node-externals": "^2.5.1",
    "@types/websocket": "1.0.0",
    "assert": "^2.0.0",
    "barrelsby": "^2.2.0",
    "buffer": "^6.0.3",
    "chai": "4.1.2",
    "eslint": "7.7.0",
    "eslint-config-airbnb-typescript-prettier": "3.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-mocha": "8.0.0",
    "eslint-plugin-more": "1.0.0",
    "global": "^4.4.0",
    "husky": "^1.3.1",
    "lint-staged": "^7.3.0",
    "mock-socket": "^8.1.1",
    "node-blob": "0.0.2",
    "node-localstorage": "^1.3.1",
    "ts-loader": "4.1.0",
    "ts-node": "8.3.0",
    "typescript": "4.1.3",
    "url": "^0.11.0",
    "util": "^0.12.3",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  }
}
